{
  "headers": [
    {
      "source": "/index.html",
      "headers": [{ "key": "Cache-Control", "value": "no-store, must-revalidate" }]
    },
    {
      "source": "/assets/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://adaptord.com/$1",
      "has": [
        {
          "type": "host",
          "value": "www.adaptord.com"
        }
      ],
      "permanent": true
    }
  ]
}
